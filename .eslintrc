{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-new": 0,
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "linebreak-style": [0, "error", "windows"]
  },
  "globals": {
    "document": false
  }
}
